<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">?attr/primaryColor</item>
        <item name="colorPrimaryDark">?attr/primaryDarkColor</item>
        <item name="colorAccent">?attr/accentColor</item>
        <item name="colorControlHighlight">?attr/rippleColor</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <item name="android:background">@color/transparent</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetDialogStyle</item>
    </style>

    <style name="AppTheme.SplashTheme">
        <item name="android:background">@drawable/background_splash</item>
    </style>

    <style name="BottomSheetDialogStyle" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:statusBarColor" tools:targetApi="21">@color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="bottomSheetStyle">@style/bottomSheetStyleWrapper</item>
    </style>

    <style name="bottomSheetStyleWrapper" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@color/transparent</item>
    </style>


