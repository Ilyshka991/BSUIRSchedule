<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/startDestination">

    <fragment
        android:id="@+id/loadInfoDestination"
        android:name="com.pechuro.bsuirschedule.feature.loadinfo.LoadInfoFragment"
        android:label="LoadInfoFragment"
        tools:layout="@layout/fragment_info_load"/>

    <fragment
        android:id="@+id/startDestination"
        android:name="com.pechuro.bsuirschedule.feature.start.StartFragment"
        android:label="StartFragment"
        tools:layout="@layout/fragment_start">
    </fragment>

    <fragment
        android:id="@+id/addScheduleDestination"
        tools:layout="@layout/fragment_add_schedule"
        android:name="com.pechuro.bsuirschedule.feature.addschedule.AddScheduleFragmentContainer"
        android:label="AddScheduleFragmentContainer" />

    <dialog
        android:id="@+id/navigationSheetDestination"
        tools:layout="@layout/fragment_navigation_sheet"
        android:name="com.pechuro.bsuirschedule.feature.navigation.NavigationSheet"
        android:label="NavigationSheet" />
</navigation>